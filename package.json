{
  "name": "betfair",
  "version": "0.7.8",
  "description": "Betfair JSON API",
  "main": "index.js",
  "private": false,
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
